<SCDConfiguration>
    <InputLevels>
        <InputLevel Name="Country"/>
        <InputLevel Name="NonEmeiaCountry" Caption="Country" Type="View"/>
        <InputLevel Name="ClusterRegion" Caption="Region"/>
        <InputLevel Name="ClusterPla" Caption="Cluster Pla"/>
        <InputLevel Name="Pla" Caption="PLA"/>
        <InputLevel Name="CentralContractGroup" Caption="Central Contract Group" HideFilter = "true"/>
        <InputLevel Name="Sfab" Caption="SFAB"/>
        <InputLevel Name="Wg" Caption="WG"/>
        <InputLevel Name="Sog" Caption="SOG"/>
        <InputLevel Name="SwDigit" Caption="SW Digit"/>
        <InputLevel Name="RoleCode" Caption="Role Code"/>

        <Default>
            <InputLevel>Country</InputLevel>
            <InputLevel>Pla</InputLevel>
            <InputLevel>CentralContractGroup</InputLevel>
            <InputLevel>Wg</InputLevel>
        </Default>
    </InputLevels>

    <RegionInputs>
        <InputLevel>Country</InputLevel>
        <InputLevel>ClusterPla</InputLevel>

        <Default>
            <InputLevel>Country</InputLevel>
        </Default>
    </RegionInputs>

    <QualityGate>
        <Default>
            <CountryGroupCoeff>0.1</CountryGroupCoeff>
            <PeriodCoeff>0.1</PeriodCoeff>
        </Default>
    </QualityGate>

    <Dependencies>
        <Dependency Name="Year"/>
        <Dependency Name="Duration"/>
        <Dependency Name="ServiceLocation" Caption="Service location"/>
        <Dependency Name="ReactionTimeType" Caption="Reaction" Type="View"/>
        <Dependency Name="ReactionTimeAvailability" Caption="Reaction" Type="View"/>
        <Dependency Name="ReactionTimeTypeAvailability" Caption="Reaction" Type="View"/>
        <Dependency Name="Availability" Caption="Availability"/>
        <Dependency Name="DurationAvailability" Caption="Duration Availability" Type="View"/>
        <Dependency Name="ProActiveSla" Caption="Pro Active SLA"/>
    </Dependencies>

    <Applications>
        <Application Name="Hardware"/>
        <Application Name="SoftwareSolution" Caption="Software &amp; Solution"/>
    </Applications>

    <Blocks>
        <Block Name="AFR">
            <Applications>
                <Application>Hardware</Application>
            </Applications>
            <Elements>
                <Element Name="AFR"  Caption="AFR (%)" InputOption="ManuallyAutomatically" RegionInput="_none_" Dependency="Year">
                    <InputLevels>
                        <InputLevel>Wg</InputLevel>
                    </InputLevels>
                    <TableView>
                        <Roles>
                            <Role>PRS PSM</Role>
                            <Role>PRS Finance</Role>
                            <Role>SCD Admin</Role>
                        </Roles>
                    </TableView>
                </Element>
            </Elements>
        </Block>

