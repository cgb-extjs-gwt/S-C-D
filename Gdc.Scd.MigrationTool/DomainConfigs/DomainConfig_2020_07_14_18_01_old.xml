<SCDConfiguration>
    <InputLevels>
        <InputLevel Id="Country"/>
        <InputLevel Id="NonEmeiaCountry" Caption="Country" Type="View"/>
        <InputLevel Id="ClusterRegion" Caption="Region"/>
        <InputLevel Id="ClusterPla" Caption="Cluster Pla"/>
        <InputLevel Id="Company" Caption="Company"/>
        <InputLevel Id="Pla" Caption="PLA"/>
        <InputLevel Id="CentralContractGroup" Caption="Central Contract Group" HideFilter = "true"/>
        <InputLevel Id="Sfab" Caption="SFAB"/>
        <InputLevel Id="Wg" Caption="WG"/>
        <InputLevel Id="Sog" Caption="SOG"/>
        <InputLevel Id="SwDigit" Caption="SW Digit"/>
        <InputLevel Id="RoleCode" Caption="Role Code"/>

        <Default>
            <InputLevel>Country</InputLevel>
            <InputLevel>Pla</InputLevel>
            <InputLevel>CentralContractGroup</InputLevel>
            <InputLevel>Wg</InputLevel>
        </Default>
    </InputLevels>

    <RegionInputs>
        <InputLevel>Country</InputLevel>
        <InputLevel>ClusterPla</InputLevel>

        <Default>
            <InputLevel>Country</InputLevel>
        </Default>
    </RegionInputs>

    <QualityGate>
        <Default>
            <CountryGroupCoeff>0.1</CountryGroupCoeff>
            <PeriodCoeff>0.1</PeriodCoeff>
        </Default>
    </QualityGate>

    <Dependencies>
        <Dependency Id="Year"/>
        <Dependency Id="Duration"/>
        <Dependency Id="ServiceLocation" Caption="Service location"/>
        <Dependency Id="ReactionTimeType" Caption="Reaction" Type="View"/>
        <Dependency Id="ReactionTimeAvailability" Caption="Reaction" Type="View"/>
        <Dependency Id="ReactionTimeTypeAvailability" Caption="Reaction" Type="View"/>
        <Dependency Id="Availability" Caption="Availability"/>
        <Dependency Id="DurationAvailability" Caption="Duration Availability" Type="View"/>
        <Dependency Id="ProActiveSla" Caption="Pro Active SLA"/>
    </Dependencies>

    <Applications>
        <Application Id="Hardware"/>
        <Application Id="SoftwareSolution" Caption="Software &amp; Solution"/>
    </Applications>

    <Tables>
        <Table Id="FieldServiceWg" Shema="Hardware" Name="FieldServiceWg"/>
        <Table Id="FieldServiceLocation" Shema="Hardware" Name="FieldServiceLocation"/>
        <Table Id="FieldServiceReactionTimeType" Shema="Hardware" Name="FieldServiceReactionTimeType"/>
        <Table Id="FieldServiceAvailability" Shema="Hardware" Name="FieldServiceAvailability"/>

        <Table Id="ProActiveCounty" Shema="Hardware" Name="ProActiveCounty"/>
