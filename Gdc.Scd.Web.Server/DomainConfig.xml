        <Element Name="CurrencyReinsurance" Caption="Currency Reinsurance" InputOption="Manually" RegionInput="_none_">
          <InputLevels>
            <InputLevel>Pla</InputLevel>
            <InputLevel>Sfab</InputLevel>
            <InputLevel>Sog</InputLevel>
            <InputLevel>SwDigit</InputLevel>
            <InputLevel>SwLicense</InputLevel>  
          </InputLevels>
          <TypeOption Type="Reference" Schema="References" Name="Currency" IdFieldName="Id" FaceFieldName="Name"/>
        </Element>
        <Element Name="RecommendedSwSpMaintenanceListPrice" Caption="Recommended SW/SP Maintenance List Price" InputOption="Manually" Dependency="Availability" RegionInput="_none_">
