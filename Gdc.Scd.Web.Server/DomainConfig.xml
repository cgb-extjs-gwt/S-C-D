    <Block Name="SwSpMaintenance" Caption="SW / SP Maintenance">
      <Applications>
        <Application>SoftwareSolution</Application>
      </Applications>
      <Elements>
        <Element Name="2ndLevelSupportCosts" Caption="2nd level support costs" InputOption="Manually" RegionInput="_none_">
          <InputLevels>
            <InputLevel>Pla</InputLevel>
            <InputLevel>Sog</InputLevel>
          </InputLevels>
        </Element>
        <Element Name="InstalledBaseSog" Caption="Installed base per SOG" InputOption="Automatically" RegionInput="_none_">
          <InputLevels>
            <InputLevel>Sog</InputLevel>
          </InputLevels>
        </Element>
        <Element Name="ReinsuranceFlatfee" Caption="Reinsurance Flatfee" InputOption="Manually" Dependency="YearAvailability" RegionInput="_none_">
          <InputLevels>
            <InputLevel>Pla</InputLevel>
            <InputLevel>Sfab</InputLevel>
            <InputLevel>Sog</InputLevel>
            <InputLevel>SwDigit</InputLevel>
            <InputLevel>SwLicense</InputLevel>  
