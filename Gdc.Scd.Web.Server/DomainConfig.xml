            <InputLevel>Sfab</InputLevel>
            <InputLevel>Sog</InputLevel>
            <InputLevel>SwDigit</InputLevel>
            <InputLevel>SwLicense</InputLevel>  
          </InputLevels>
        </Element>
        <Element Name="MarkupForProductMarginSwLicenseListPrice" Caption="Markup for Product Margin of SW License List Price" InputOption="Manually" Dependency="Availability" RegionInput="_none_">
          <InputLevels>
            <InputLevel>Pla</InputLevel>
            <InputLevel>Sfab</InputLevel>
            <InputLevel>Sog</InputLevel>
            <InputLevel>SwDigit</InputLevel>
            <InputLevel>SwLicense</InputLevel>  
          </InputLevels>
        </Element>
        <Element Name="ShareSwSpMaintenanceListPrice" Caption="Share SW/SP Maintenance List Price" InputOption="Manually" Dependency="Availability" RegionInput="_none_">
          <InputLevels>
            <InputLevel>Pla</InputLevel>
            <InputLevel>Sfab</InputLevel>
            <InputLevel>Sog</InputLevel>
            <InputLevel>SwDigit</InputLevel>
            <InputLevel>SwLicense</InputLevel>  
          </InputLevels>
        </Element>
        <Element Name="DiscountDealerPrice" Caption="Discount to Dealer price in %" InputOption="Manually" RegionInput="_none_">
          <InputLevels>
            <InputLevel>Pla</InputLevel>
            <InputLevel>Sfab</InputLevel>
            <InputLevel>Sog</InputLevel>
            <InputLevel>SwDigit</InputLevel>
            <InputLevel>SwLicense</InputLevel>  
          </InputLevels>
        </Element>
      </Elements>
