<SCDConfiguration>
  <InputLevels>
    <InputLevel Name="Country"/>
    <InputLevel Name="ClusterRegion" Caption="Region"/>
    <InputLevel Name="ClusterPla" Caption="Cluster Pla"/>
    <InputLevel Name="Pla" Caption="PLA"/>
    <InputLevel Name="Sfab" Caption="SFAB"/>
    <InputLevel Name="Sog" Caption="SOG"/>
    <InputLevel Name="SwDigit" Caption="SW Level (Digit)"/>
    <InputLevel Name="SwLicense" Caption="SW License"/>
    <InputLevel Name="Wg" Caption="WG"/>
    <InputLevel Name="RoleCode" Caption="Role Code"/>
    
    <Default>
      <InputLevel>Country</InputLevel>
      <InputLevel>Pla</InputLevel>
      <InputLevel>Wg</InputLevel>
    </Default>
  </InputLevels>

  <RegionInputs>
    <InputLevel>Country</InputLevel>
    <InputLevel>ClusterPla</InputLevel>
      
    <Default>
      <InputLevel>Country</InputLevel>
    </Default>
  </RegionInputs>

  <QualityGate>
    <Default>
      <CountryGroupCoeff>0.1</CountryGroupCoeff>
      <PeriodCoeff>0.1</PeriodCoeff>
    </Default>
  </QualityGate>
  
  <Dependencies>
    <Dependency Name="Year"/>
    <Dependency Name="ServiceLocation" Caption="Service location"/>
    <Dependency Name="ReactionTimeType" Caption="Reaction" Type="View"/>
    <Dependency Name="ReactionTimeAvailability" Caption="Reaction" Type="View"/>
    <Dependency Name="ReactionTimeTypeAvailability" Caption="Reaction" Type="View"/>
    <Dependency Name="Availability" Caption="Availability"/>
    <Dependency Name="YearAvailability" Caption="Year Availability" Type="View"/>
    <Dependency Name="ProActiveSla" Caption="Pro Active SLA"/>
  </Dependencies>

  <Applications>
    <Application Name="Hardware"/>
    <Application Name="SoftwareSolution" Caption="Software &amp; Solution"/>
    <Application Name="Atom"/>
  </Applications>
  
  <Blocks>
    <Block Name="AFR">
      <Applications>
        <Application>Atom</Application>
      </Applications>
      <Elements>
        <Element Name="AFR" InputOption="ManualyAutomaticly" RegionInput="_none_" Dependency="Year">
          <InputLevels>
            <InputLevel>Wg</InputLevel>
          </InputLevels>
          <TableView>
            <Roles>
              <Role>PRS PSM</Role>  
              <Role>PRS Finance</Role>
              <Role>SCD Admin</Role>
            </Roles>
          </TableView>
        </Element>
      </Elements>
    </Block>

    <Block Name="InstallBase" Caption="Installed base">
      <Applications>
        <Application>Atom</Application>
      </Applications>
      <Elements>
        <Element Name="InstalledBaseCountry" Caption="Installed base country" InputOption="ManualyAutomaticly"/>
      </Elements>
    </Block>

    <Block Name="MarkupOtherCosts" Caption="Markup other costs">
      <Applications>
        <Application>Atom</Application>
      </Applications>
      <Elements>
        <Element Name="MarkupFactor" Caption="Markup factor for other cost" InputOption="Manually" Dependency="ReactionTimeTypeAvailability"/>
        <Element Name="Markup" Caption="Markup for other cost" InputOption="Manually" Dependency="ReactionTimeTypeAvailability"/>
      </Elements>
    </Block>

    <Block Name="ProlongationMarkup" Caption="Prolongation markup">
      <Applications>
        <Application>Atom</Application>
      </Applications>
      <Elements>
        <Element Name="ProlongationMarkupFactor" Caption="Prolongation markup factor for other cost" InputOption="Manually" Dependency="ReactionTimeTypeAvailability"/>
        <Element Name="ProlongationMarkup" Caption="Prolongation markup for other cost" InputOption="Manually" Dependency="ReactionTimeTypeAvailability"/>
      </Elements>
    </Block>

    <Block Name="MarkupStandardWaranty" Caption="Markup standard waranty">
      <Applications>
        <Application>Atom</Application>
      </Applications>
      <Elements>
        <Element Name="MarkupFactorStandardWarranty" Caption="Markup factor for standard warranty local cost" InputOption="Manually" Dependency="ReactionTimeTypeAvailability"/>
        <Element Name="MarkupStandardWarranty" Caption="Markup for standard warranty local cost" InputOption="Manually" Dependency="ReactionTimeTypeAvailability"/>
      </Elements>
    </Block>

    <Block Name="MaterialCostWarranty" Caption="Material cost iW" >
      <Applications>
        <Application>Atom</Application>
      </Applications>
      <Elements>
        <Element Name="MaterialCostWarranty" Caption="Material cost iW" InputOption="ManualyAutomaticly" RegionInput="ClusterRegion">
          <InputLevels>
            <InputLevel>Wg</InputLevel>
          </InputLevels>
          <!--<TableView>
            <Roles>
              <Role>PRS PSM</Role>  
              <Role>PRS Finance</Role>
            </Roles>
          </TableView>-->
        </Element>
      </Elements>
    </Block>

    <Block Name="MaterialCostOow" Caption="Material cost OOW">
      <Applications>
        <Application>Atom</Application>
      </Applications>
      <Elements>
        <Element Name="MaterialCostOow" Caption="Material cost OOW" InputOption="Manually" RegionInput="ClusterRegion">
          <InputLevels>
            <InputLevel>Wg</InputLevel>
          </InputLevels>
          <!--<TableView>
            <Roles>
              <Role>PRS PSM</Role>  
              <Role>PRS Finance</Role>
            </Roles>
          </TableView>-->
        </Element>
      </Elements>
    </Block>

