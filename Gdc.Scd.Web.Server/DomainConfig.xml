    <Block Name="RoleCodeHourlyRates" Caption="Role Code Hourly Rates">
      <Applications>
        <Application>Atom</Application>
      </Applications>
      <Elements>
        <Element Name="OnsiteHourlyRates" Caption="Onsite Hourly Rates" InputOption="ManualyAutomaticly" RegionInput="_none_">
          <InputLevels>
            <InputLevel>RoleCode</InputLevel>
          </InputLevels>
        </Element>
      </Elements>
    </Block>
      
    <Block Name="FieldServiceCost" Caption="Field Service Cost">
      <Applications>
        <Application>Hardware</Application>
      </Applications>
      <Elements>
        <Element Name="RepairTime" Caption="Repair Time (MTTR)" InputOption="Manually" RegionInput="_none_">
          <InputLevels>
            <InputLevel>Pla</InputLevel>
            <InputLevel>Wg</InputLevel>
          </InputLevels>
          <TableView>
            <Roles>
              <Role>PRS PSM</Role>
              <Role>SCD Admin</Role>
            </Roles>
          </TableView>
        </Element>
        <Element Name="TravelTime" Caption="Travel Time (MTTT)" InputOption="Manually" Dependency="ServiceLocation"/>
        <!--<Element Name="OnsiteHourlyRate" Caption="Onsite hourly rate" InputOption="Reference"/>-->
        <Element Name="LabourCost" Caption="Labour cost" InputOption="Manually" Dependency="ServiceLocation"/>
        <Element Name="TravelCost" Caption="Travel cost" InputOption="Manually" Dependency="ServiceLocation"/>
        <Element Name="PerformanceRate" Caption="Performance rate" InputOption="Manually" Dependency="ReactionTimeType"/>
        <Element Name="TimeAndMaterialShare" Caption="Time and material share" InputOption="Manually"/>
        <!--<Element Name="AFR" InputOption="Reference"/>-->
      </Elements>
      <QualityGate>
        <CountryGroupCoeff>0.15</CountryGroupCoeff>
        <PeriodCoeff>0.15</PeriodCoeff>
      </QualityGate>
    </Block>

    <Block Name="ServiceSupportCost" Caption="Service Support Cost">
      <Applications>
        <Application>Hardware</Application>
      </Applications>
      <Elements>
        <Element Name="1stLevelSupportCostsCountry" Caption="1st level support costs country" InputOption="Manually">
          <InputLevels>
            <InputLevel>Country</InputLevel>
          </InputLevels>
        </Element>
        <!--<Element Name="InstalledBase" Caption="Installed base country" InputOption="Reference"/>-->
        <Element Name="2ndLevelSupportCostsClusterRegion" Caption="2nd Level Support costs cluster region" InputOption="Manually" RegionInput="ClusterRegion">
          <InputLevels>
            <InputLevel>ClusterPla</InputLevel>
          </InputLevels>
        </Element>
        <Element Name="2ndLevelSupportCostsLocal" Caption="2nd Level Support costs local" InputOption="Manually" RegionInput="ClusterRegion">
          <InputLevels>
            <InputLevel>ClusterPla</InputLevel>
          </InputLevels>
        </Element>
        <!--<Element Name="InstalledBasePla" Caption="Installed base PLA" InputOption="Reference"/>-->
      </Elements>
    </Block>

    <Block Name="LogisticsCosts" Caption="Logistics Costs">
      <Applications>
        <Application>Hardware</Application>
      </Applications>
      <Elements>
        <Element Name="StandardHandling" Caption="Standard handling in country" InputOption="Manually" Dependency="ReactionTimeType"/>
        <Element Name="HighAvailabilityHandling" Caption="High availability handling in country" InputOption="Manually" Dependency="ReactionTimeType"/>
        <Element Name="StandardDelivery" Caption="Standard delivery" InputOption="Manually" Dependency="ReactionTimeType"/>
        <Element Name="ExpressDelivery" Caption="Express delivery" InputOption="Manually" Dependency="ReactionTimeType"/>
        <Element Name="TaxiCourierDelivery" Caption="Taxi/courier delivery" InputOption="Manually" Dependency="ReactionTimeType"/>
        <Element Name="ReturnDeliveryFactory" Caption="Return delivery to factory" InputOption="Manually" Dependency="ReactionTimeType"/>
        <!--<Element Name="AFR" InputOption="Reference"/>-->
      </Elements>
    </Block>

    <Block Name="AvailabilityFee" Caption="Availability fee">
      <Applications>
        <Application>Hardware</Application>
      </Applications>
      <Elements>
        <Element Name="InstalledBaseHighAvailability" Caption="Installed base high availability" InputOption="Manually"/>
        <Element Name="TotalLogisticsInfrastructureCost" Caption="Total logistics infrastructure cost" InputOption="Manually">
          <InputLevels>
            <InputLevel>Country</InputLevel>
          </InputLevels>
        </Element>
        <Element Name="StockValueFj" Caption="Stock value in country FJ" InputOption="Manually">
          <InputLevels>
            <InputLevel>Country</InputLevel>
          </InputLevels>
        </Element>
        <Element Name="StockValueMv" Caption="Stock value in country MV" InputOption="Manually">
          <InputLevels>
            <InputLevel>Country</InputLevel>
          </InputLevels>
        </Element>
        <Element Name="AverageContractDuration" Caption="Average contract duration" InputOption="Manually">
          <InputLevels>
            <InputLevel>Country</InputLevel>
          </InputLevels>
        </Element>
        <Element Name="CostPerKit" Caption="Cost per KIT" InputOption="Automatically" RegionInput="_none_"/>
        <Element Name="CostPerKitJapanBuy" Caption="Cost per KIT Japan-Buy" InputOption="Automatically" RegionInput="_none_"/>
        <Element Name="MaxQty" Caption="MaxQty" InputOption="Automatically" RegionInput="_none_"/>
        <Element Name="JapanBuy" Caption="Japan Buy" InputOption="Manually">
          <TypeOption Type="Flag"/>
        </Element>
      </Elements>
    </Block>

    <Block Name="HddRetention" Caption="HDD Retention">
      <Applications>
        <Application>Hardware</Application>
      </Applications>
      <Elements>
        <Element Name="HddFr" Caption="HDD FR" InputOption="Manually" Dependency="Year" RegionInput="_none_">
          <InputLevels>
            <InputLevel>Wg</InputLevel>
          </InputLevels>
          <TableView>
            <Roles>
              <Role>PRS PSM</Role>  
            </Roles>
          </TableView>
        </Element>
        <Element Name="HddMaterialCost" Caption="HDD Material Cost" InputOption="Manually" RegionInput="_none_">
          <InputLevels>
            <InputLevel>Wg</InputLevel>
          </InputLevels>
          <TableView>
            <Roles>
              <Role>PRS PSM</Role>  
            </Roles>
          </TableView>
        </Element>
      </Elements>
    </Block>

    <Block Name="Reinsurance">
      <Applications>
        <Application>Hardware</Application>
      </Applications>
      <Elements>
        <Element Name="CurrencyReinsurance" Caption="Currency Reinsurance" InputOption="Manually" Dependency="Year" RegionInput="_none_">
          <InputLevels>
            <InputLevel>Wg</InputLevel>
          </InputLevels>
          <TypeOption Type="Reference" Schema="References" Name="Currency" IdFieldName="Id" FaceFieldName="Name"/>
          <TableView>
            <Roles>
              <Role>PRS PSM</Role>  
            </Roles>
          </TableView>
        </Element>
        <Element Name="ReinsuranceFlatfee" Caption="Reinsurance Flatfee" InputOption="Manually" RegionInput="_none_" Dependency="Year">
          <InputLevels>
            <InputLevel>Wg</InputLevel>
          </InputLevels>
          <TableView>
            <Roles>
              <Role>PRS PSM</Role>  
            </Roles>
          </TableView>
        </Element>
        <Element Name="ReinsuranceUpliftFactor" Caption="Reinsurance uplift factor" InputOption="Manually" Dependency="ReactionTimeAvailability" RegionInput="_none_">
          <InputLevels>
            <InputLevel>Wg</InputLevel>
          </InputLevels>
          <TableView>
            <Roles>
              <Role>PRS PSM</Role>  
            </Roles>
          </TableView>
        </Element>
      </Elements>
    </Block>

