            </Roles>
          </TableView>
        </Element>
        <Element Name="LocalRemoteShcCustomerBriefingEffort" Caption="Local remote SHC customer briefing effort" InputOption="Manually"/>
        <Element Name="LocalOnSiteShcCustomerBriefingEffort" Caption="Local on-site SHC customer briefing effort" InputOption="Manually"/>
        <Element Name="TravellingTime" Caption="Travelling Time (MTTT)" InputOption="Manually"/>
        <Element Name="OnSiteHourlyRate" Caption="On-Site Hourly Rate" InputOption="Manually"/>
      </Elements>
    </Block>
    
    <Block Name="ProActiveSw" Caption="ProActive">
