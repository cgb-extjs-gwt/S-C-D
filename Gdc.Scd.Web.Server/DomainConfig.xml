      <Elements>
        <Element Name="LocalRemoteAccessSetupPreparationEffort" Caption="Local Remote-Access setup preparation effort" InputOption="Manually"/>
        <Element Name="LocalRegularUpdateReadyEffort" Caption="Local regular update ready for service effort" InputOption="Manually"/>
        <Element Name="LocalPreparationShcEffort" Caption="Local preparation SHC effort" InputOption="Manually"/>
        <Element Name="CentralExecutionShcReportCost" Caption="Central execution SHC &amp; report cost" InputOption="Manually" RegionInput="_none_">
          <InputLevels>
            <InputLevel>Pla</InputLevel>
            <InputLevel>Wg</InputLevel>
          </InputLevels>
          <TableView>
            <Roles>
              <Role>PRS PSM</Role>  
